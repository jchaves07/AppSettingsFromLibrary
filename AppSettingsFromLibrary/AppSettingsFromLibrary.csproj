<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackOnBuild>true</PackOnBuild>
    <PackageId>AppSettings_Read_from_Library</PackageId>
    <Authors>Jorge Chaves</Authors>
    <Copyright>jojodev</Copyright>
    <DevelopmentDependency>true</DevelopmentDependency>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Owners>Jorge Chaves</Owners>
    <Summary>AppSettings_Read_from_Library</Summary>
    <Title>AppSettings_Read_from_Library</Title>
    <Description>Read AppSettings.json of asp.net core MVC proyect from other library.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Microsoft.Extensions.Configuration" />
    <None Remove="Microsoft.Extensions.Configuration.Json" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="5.0.0" />
  </ItemGroup>
</Project>
